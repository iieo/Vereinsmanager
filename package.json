{
  "name": "vereinsmanager",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "description": "Manage the members and bills",
  "author": "iieo <achtbyte01@gmail.com>",
  "keywords": [
    "react",
    "expo",
    "template",
    "nativebase"
  ],
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "prod": "yarn electron-webpack && yarn electron-builder --dir -c.mac.identity=null --linux deb"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "3.5.2",
    "expo": "~42.0.0",
    "expo-asset": "~8.3.3",
    "expo-file-system": "~11.1.3",
    "expo-status-bar": "~1.0.4",
    "firebase": "^9.6.9",
    "native-base": "^3.3.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-native-paper": "^4.11.2",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "^0.15.7",
    "url-loader": "^4.1.1",
    "webpack": "4.42.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.16.19",
    "babel-loader": "^8.2.4",
    "electron": "^18.0.3",
    "electron-builder": "^22.14.13"
  },
  "bugs": {
    "url": "https://github.com/GeekyAnts/nativebase-templates/issues"
  },
  "homepage": "https://github.com/GeekyAnts/nativebase-templates#readme",
  "private": true,
  "build": {
    "appId": "de.bauerle.vereinsmanager",
    "artifactName": "Vereinsmanager-${version}-${os}-${arch}.${ext}",
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  }
}
